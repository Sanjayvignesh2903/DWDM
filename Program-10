speeds <- c(78.3, 81.8, 82, 74.2, 83.4, 84.5, 82.9, 77.5, 80.9, 70.6)

# Calculate Standard Deviation
sd_val <- sd(speeds)

# Calculate Interquartile Range (IQR)
iqr_val <- IQR(speeds)

# Print the results
cat("Standard Deviation:", round(sd_val, 4), "\n")
cat("Interquartile Range (IQR):", iqr_val, "\n")
