pencils <- c(9, 25, 23, 12, 11, 6, 7, 8, 9, 10)

mean_pencils <- mean(pencils)
median_pencils <- median(pencils)

# Mode calculation (R does not have a built-in mode function for numbers)
pencil_counts <- table(pencils)
mode_pencils <- as.numeric(names(pencil_counts[pencil_counts == max(pencil_counts)]))

cat("Mean:   ", mean_pencils, "\n")
cat("Median: ", median_pencils, "\n")
cat("Mode:   ", mode_pencils, "\n")
