age <- c(23, 23, 27, 27, 39, 41, 47, 49, 50, 52, 54, 54, 56, 57, 58, 58, 60, 61)
v <- 35

# (i) Min-max normalization
min_max <- (v - min(age)) / (max(age) - min(age))

# (ii) Z-score normalization
z_score <- (v - mean(age)) / 12.94

# (iii) Decimal scaling
j <- ceiling(log10(max(age)))
decimal_scaling <- v / (10^j)

cat("Min-Max Normalization (35):", min_max, "\n")
cat("Z-Score Normalization (35):", z_score, "\n")
cat("Decimal Scaling Normalization (35):", decimal_scaling, "\n")
